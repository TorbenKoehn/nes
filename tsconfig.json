{
    "compileOnSave": false,
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "target": "es6",
        "moduleResolution": "node",
        "strict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "sourceMap": true,
        "skipLibCheck": true,
        "allowJs": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "experimentalDecorators": true,
        "jsx": "preserve"
    }
}